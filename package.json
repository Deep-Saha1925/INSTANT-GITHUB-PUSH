{
  "name": "giteasy",
  "displayName": "GitEasy",
  "publisher": "DeepSaha-1925",
  "description": "Push your VS Code workspace to GitHub in one click.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createAndPushRepo"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createAndPushRepo",
        "title": "Create & Push to GitHub"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "open": "^10.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepSaha-1925/giteasy.git"
  },
  "license": "MIT"
}
